  "dependencies": {
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "adm-zip": "^0.5.10",
    "async-sema": "^3.1.1",
    "canvas": "^2.8.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "gif-encoder-2": "^1.0.5",
    "multer": "^1.4.5-lts.1",
    "next": "latest",
    "node-fetch": "^2.6.6",
    "nodemon": "^3.0.1",
    "puppeteer": "^13.4.1",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-adblocker": "^2.12.0",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
